{
  "name": "material-dashboard-angular",
  "version": "2.8.33",
  "description": "Desktop application for Tradewest",
  "author": {
    "name": "Conor Scott",
    "email": "conorpscott@outlook.com"
  },
  "license": "MIT",
  "main": "main.js",
  "overrides": {
    "lmdb": "0.0.0"
  },
  "build": {
    "productName": "Tradewest App",
    "files": [
      "main.js",
      "preload.js",
      "dist/**",
      "node_modules/**",
      "package.json"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:clean": "rimraf node_modules package-lock.json && npm install && ng serve -o",
    "preinstall": "npx npm-force-resolutions",
    "electron": "electron .",
    "dist": "electron-builder --win --x64 --publish=always",
    "package-win": "electron-packager . --platform=win32 --arch=x64 --out=dist --overwrite --icon=src/assets/img/tradewest-mobile-full.ico",
    "electron:build": "electron-builder",
    "rebuild": "electron-rebuild -f -w lmdb"
  },
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "rebuildConfig": {
    "force": true,
    "disabledModules": [
      "lmdb"
    ],
    "modules": []
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1802.5",
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular-devkit/core": "^18.2.5",
    "@angular/animations": "^18.2.5",
    "@angular/cdk": "^14.2.0",
    "@angular/cli": "^18.2.5",
    "@angular/common": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/elements": "^18.2.5",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.5",
    "@angular/localize": "^18.2.5",
    "@angular/material": "^14.2.0",
    "@angular/material-date-fns-adapter": "^14.2.7",
    "@angular/material-moment-adapter": "^18.2.6",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@getbrevo/brevo": "^1.0.1",
    "ajv": "8.11.0",
    "arrive": "2.4.1",
    "bootstrap": "^5.3.3",
    "bootstrap-material-design": "^4.1.3",
    "bootstrap-notify": "3.1.3",
    "chartist": "0.11.4",
    "classlist.js": "1.1.20150312",
    "dotenv": "^16.4.5",
    "electron-updater": "^6.3.9",
    "eslint": "^8.11.0",
    "express": "^4.21.0",
    "find-up": "^7.0.0",
    "firebase": "^10.14.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "googleapis": "99.0.0",
    "hammerjs": "2.0.8",
    "jquery": "3.6.0",
    "moment": "^2.30.1",
    "pdfmake": "^0.2.12",
    "perfect-scrollbar": "1.5.5",
    "pkg-dir": "^8.0.0",
    "popper.js": "1.16.1",
    "rxjs": "7.5.7",
    "sib-api-v3-sdk": "^8.5.0",
    "tslib": "^2.3.0",
    "web-animations-js": "2.3.2",
    "zone.js": "~0.14.10"
  },
  "resolutions": {
    "rxjs": "7.5.7",
    "**/lmdb": "2.4.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^18.2.5",
    "@angular/language-service": "18.2.5",
    "@types/bootstrap": "4.5.0",
    "@types/chartist": "0.11.1",
    "@types/google.maps": "3.47.4",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/jquery": "3.5.6",
    "@types/node": "^22.5.5",
    "electron": "^24.3.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "fs-extra": "^10.1.0",
    "jasmine-core": "~4.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss-loader": "^8.1.1",
    "protractor": "^7.0.0",
    "sass": "1.32.13",
    "sass-loader": "^16.0.2",
    "ts-node": "~10.9.1",
    "typescript": "5.5.2"
  }
}
